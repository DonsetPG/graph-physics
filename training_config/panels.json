{
    "dataset": {
        "extension": "xdmf",
        "xdmf_folder": "dataset/train",
        "meta_path": "dataset/meta.json",
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 15,
        "hidden_size": 128,
        "node_input_size": 2,
        "output_size": 3,
        "edge_input_size": 0,
        "num_heads": 4
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 2,
        "output_index_start": 0,
        "output_index_end": 3,
        "node_type_index": 3
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.02,
            "noise_index_start": [0],
            "noise_index_end": [2],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    }
}