{
    "dataset": {
        "extension": "h5",
        "h5_path": "dataset/h5_dataset/deforming_plate/train.h5",
        "meta_path": "dataset_config/plate_meta.json",
        "targets": ["world_pos"],
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 10,
        "node_input_size": 9,
        "edge_input_size": 0,
        "output_size": 3,
        "hidden_size": 64,
        "num_heads": 4
    },
    "features": {
        "node": ["world_pos", "obstacle_displacement", "mesh_pos", "node_type"],
        "node_type": "node_type"
    },
    "sizes": {
        "obstacle_displacement": 3
    },
    "named_features": {
        "mode": "semantic",
        "legacy_index": {
            "feature_index_start": 0,
            "feature_index_end": 9,
            "output_index_start": 0,
            "output_index_end": 3,
            "node_type_index": 9
        }
    },
    "transformations": {
        "preprocessing": {
            "noise_scale": 0.003,
            "noise_features": ["world_pos"],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": true,
            "world_pos_feature": "world_pos",
            "target_feature": "world_pos",
            "displacement_feature": "obstacle_displacement",
            "node_type_feature": "node_type"
        }
    }
}
