{
    "dataset": {
        "extension": "obj",
        "obj_folder": "dataset/train", 
        "meta_path": "dataset/meta.json",
        "khop": 1
    },
    "model": {
        "type": "classification",
        "message_passing_num": 10,
        "hidden_size": 128,
        "node_input_size": 1,
        "output_size": 1,
        "edge_input_size": 0,
        "num_heads": 4
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 1,
        "output_index_start": 0,
        "output_index_end": 1,
        "node_type_index": 1
    },
    "transformations": {
        "preprocessing": {
            "noise": [10.0],
            "noise_index_start": [0],
            "noise_index_end": [1],
            "masking": 0.1
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    }
}
