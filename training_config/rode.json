{
    "dataset": {
        "extension": "xdmf",
        "xdmf_folder": "dataset/train",
        "meta_path": "dataset_config/rode.json",
        "khop": 1
    },
    "model": {
        "type": "epd",
        "message_passing_num": 15,
        "hidden_size": 128,
        "node_input_size": 9,
        "output_size": 3,
        "edge_input_size": 4,
        "num_heads": 0,
        "use_silu_activation": false,
        "use_rope_embeddings": false,
        "use_gated_attention": false
    },
    "loss": {
        "type": ["l2loss", "volumeloss"],
        "weights": [0.95, 0.05]
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 9,
        "output_index_start": 0,
        "output_index_end": 3,
        "node_type_index": 9
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.002,
            "noise_index_start": [0],
            "noise_index_end": [3],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    },
    "training": {
        "use_spatial_mtp": false,
        "use_temporal_block": false
    }
}
