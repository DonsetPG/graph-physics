{
    "dataset": {
        "extension": "xdmf",
        "xdmf_folder": "dataset/train",
        "meta_path": "dataset/meta.json",
        "inputs": ["Vitesse_4D_Flow"],
        "targets": ["Vitesse"],
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 10,
        "hidden_size": 64,
        "node_input_size": 6,
        "output_size": 3,
        "edge_input_size": 0,
        "num_heads": 4,
        "use_silu_activation": false,
        "use_rope_embeddings": false,
        "use_gated_attention": false
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 6,
        "output_index_start": 0,
        "output_index_end": 3,
        "node_type_index": 6
    },
    "loss": {
        "type": ["l2loss"],
        "weights": [1.0],
        "gradient_method": "finite_diff"
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.0,
            "noise_index_start": [0, 1, 2, 4, 5, 6],
            "noise_index_end": [1, 2, 3, 5, 6, 7],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    },
    "training": {
        "use_spatial_mtp": false,
        "use_temporal_block": false
    }
}
