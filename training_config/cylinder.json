{
    "dataset": {
        "extension": "h5",
        "h5_path": "dataset/h5_dataset/cylinder_flow/train.h5",
        "meta_path": "dataset/h5_dataset/cylinder_flow/meta.json",
        "khop": 1
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 5,
        "hidden_size": 32,
        "node_input_size": 4,
        "output_size": 2,
        "edge_input_size": 0,
        "num_heads": 4
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 2,
        "output_index_start": 0,
        "output_index_end": 2,
        "node_type_index": 2
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.02,
            "noise_index_start": [0],
            "noise_index_end": [2],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    }
}