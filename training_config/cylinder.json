{
    "dataset": {
        "extension": "h5",
        "h5_path": "dataset/h5_dataset/cylinder_flow/train.h5",
        "meta_path": "dataset_config/cylinder_meta.json",
        "targets": ["velocity"],
        "khop": 1
    },
    "model": {
        "type": "epd",
        "message_passing_num": 5,
        "hidden_size": 32,
        "node_input_size": 2,
        "output_size": 2,
        "edge_input_size": 3,
        "use_silu_activation": false,
        "use_gated_mlp": false
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 2,
        "output_index_start": 0,
        "output_index_end": 2,
        "node_type_index": 2
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.02,
            "noise_index_start": [0],
            "noise_index_end": [2],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    },
    "training": {
        "use_spatial_mtp": false,
        "use_temporal_block": false
    }
}
