{
    "dataset": {
        "extension": "xdmf",
        "xdmf_folder": "dataset-shell-frame/test",
        "meta_path": "dataset-shell-frame/meta.json",
        "target_same_frame": true,
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 15,
        "hidden_size": 128,
        "node_input_size": 1111,
        "output_size": 1,
        "edge_input_size": 0,
        "num_heads": 4,
        "use_silu_activation": false,
        "use_rope_embeddings": false,
        "use_gated_attention": false
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 1111,
        "output_index_start": 0,
        "output_index_end": 1,
        "node_type_index": 86
    },
    "transformations": {
        "preprocessing": {
            "noise": [],
            "noise_index_start": [],
            "noise_index_end": [],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    },
    "training": {
        "use_spatial_mtp": false,
        "use_temporal_block": false
    }
}
